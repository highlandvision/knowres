<?xml version="1.0" encoding="utf-8"?>
<form addruleprefix="HighlandVision\Component\Knowres\Administrator\Rule">
	<fieldset name="custom">
		<field name="url"
			default="https://api.beyondpricing.com/v1/"
			filter="string"
			required="true"
			type="url"
			label="COM_KNOWRES_SERVICE_SERVICE_URL_LBL"
			description="COM_KNOWRES_SERVICE_SERVICE_URL_DSC"/>
		<field name="apikey"
			filter="string"
			required="true"
			type="text"
			label="COM_KNOWRES_SERVICE_APIKEY_LBL"
			description="COM_KNOWRES_SERVICE_APIKEY_DSC"/>
		<field name="channel"
			filter="string"
			required="true"
			type="list"
			label="COM_KNOWRES_SERVICE_CHANNEL_LBL"
			description="COM_KNOWRES_SERVICE_CHANNEL_DSC">
			<option value="airbnb">airbnb</option>
		</field>
		<field name="account_email"
			filter="string"
			required="true"
			type="email"
			label="COM_KNOWRES_SERVICE_ACCOUNT_EMAIL_LBL"
			description="COM_KNOWRES_SERVICE_ACCOUNT_EMAIL_DSC">
		</field>
	</fieldset>
</form>