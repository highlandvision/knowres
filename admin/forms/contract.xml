<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="HighlandVision\Component\Knowres\Administrator\Field"
      addruleprefix="HighlandVision\Component\Knowres\Administrator\Rule">
	<fieldset name="krdates">
		<field name="arrival_bd"
		       type="text"
		       class="input-mini px-2 pb-1"
		       hiddenDescription="true"
		       readonly="true"
		       label="COM_KNOWRES_CONTRACT_ARRIVAL_LBL"
		       description="COM_KNOWRES_CONTRACT_ARRIVAL_DSC"/>
		<field name="departure_bd"
		       type="text"
		       class="input-mini px-2 pb-1"
		       hiddenDescription="true"
		       readonly="true"
		       label="COM_KNOWRES_CONTRACT_DEPARTURE_LBL"
		       description="COM_KNOWRES_CONTRACT_DEPARTURE_DSC"/>
		<field name="guests"
		       type="hidden"
		       class="px-2 pb-1"
		       default="2"
		       filter="integer"
		       label="COM_KNOWRES_CONTRACT_GUESTS_LBL"/>
		<field name="adults"
		       type="number"
		       class="kr-calculate px-2 pb-1"
		       default="2"
		       filter="integer"
		       min="1"
		       max="16"
		       required="true"
		       step="1"
		       label="COM_KNOWRES_CONTRACT_GUESTS_ADULTS"/>
		<field name="children"
		       type="number"
		       class="kr-calculate px-2 pb-1"
		       default="0"
		       filter="integer"
		       min="0"
		       max="17"
		       step="1"
		       label="COM_KNOWRES_CONTRACT_GUESTS_CHILDREN"
		       description="COM_KNOWRES_CONTRACT_GUESTS_CHILDREN_DSC"/>
		<field name="child_ages"
		       type="text"
		       class="kr-calculate px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_GUESTS_AGES"
		       description="COM_KNOWRES_CONTRACT_GUESTS_AGES_DSC"/>
		<field name="fixrate"
		       type="list"
		       class="px-2 pb-1"
		       default="0"
		       hiddenDescription="true"
		       label="COM_KNOWRES_CONTRACT_FIXRATE_LBL"
		       description="COM_KNOWRES_CONTRACT_FIXRATE_DSC">
			<option value="0">JNO</option>
			<option value="1">JYES</option>
		</field>
	</fieldset>
	<fieldset name="krcoupon">
		<field name="coupon_code"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       hiddenLabel="true"
		       label="COM_KNOWRES_CONTRACT_COUPON_LBL"
		       description="COM_KNOWRES_CONTRACT_COUPON_DSC"/>
	</fieldset>
	<fieldset name="krextras">
		<field name="extras"
		       type="jsonextras"
		       class="px-2 pb-1"
		       filter="string"
		       hiddenLabel="true"
		       hiddenDescription="true"
		       label="COM_KNOWRES_CONTRACT_EXTRAS_LBL"
		       description="COM_KNOWRES_CONTRACT_EXTRAS_DSC"/>
	</fieldset>
	<fieldset name="krdateoptions">
		<field name="expiry_days"
		       type="number"
		       class="kr-calculate px-2 pb-1"
		       default="2"
		       min="0"
		       max="30"
		       step="1"
		       label="COM_KNOWRES_CONTRACT_EXPIRY_DAYS_LBL"
		       description="COM_KNOWRES_CONTRACT_EXPIRY_DAYS_DSC"/>
		<field name="balance_days"
		       type="number"
		       class="kr-calculate px-2 pb-1"
		       default="0"
		       filter="integer"
		       min="0"
		       max="120"
		       step="1"
		       label="COM_KNOWRES_CONTRACT_BALANCE_DAYS_LBL"
		       description="COM_KNOWRES_CONTRACT_BALANCE_DAYS_DSC"/>
		<field name="net_price"
		       type="text"
		       class="kr-calculate input-mini px-2 pb-1"
		       filter="string"
		       validate="netprice"
		       label="COM_KNOWRES_CONTRACT_NET_PRICE_LBL"
		       description="COM_KNOWRES_CONTRACT_NET_PRICE_DSC"/>
	</fieldset>
	<fieldset name="krguest">
		<field name="guest_id"
		       type="text"
		       class="input-mini px-2 pb-1"
		       default="0"
		       filter="integer"
		       label="COM_KNOWRES_GUEST"
		       description="COM_KNOWRES_CONTRACT_GUEST_ID_DSC"/>
		<field name="guest_types"
		       type="text"
		       class="px-2 pb-1"
		       label="COM_KNOWRES_CONTRACT_GUEST_TYPES_LBL"
		       description="COM_KNOWRES_CONTRACT_GUEST_TYPES_DSC"/>
		<field name="rooms"
		       type="text"
		       class="px-2 pb-1"
		       label="COM_KNOWRES_CONTRACT_ROOMS_LBL"
		       description="COM_KNOWRES_CONTRACT_ROOMS_DSC"/>
	</fieldset>
	<fieldset name="krnotes">
		<field name="guest_note"
		       type="textarea"
		       filter="string"
		       hiddenLabel="true"
		       rows="5"
		       label="COM_KNOWRES_CONTRACT_GUEST_NOTE_LBL"
		       description="COM_KNOWRES_CONTRACT_GUEST_NOTE_DSC"/>
		<field name="owner_note"
		       type="textarea"
		       filter="string"
		       hiddenLabel="true"
		       rows="5"
		       label="COM_KNOWRES_CONTRACT_OWNER_NOTE_LBL"
		       description="COM_KNOWRES_CONTRACT_OWNER_NOTE_DSC"/>
	</fieldset>
	<fieldset name="krblocknote">
		<field name="block_note"
		       type="textarea"
		       filter="string"
		       rows="5"
		       label="COM_KNOWRES_CONTRACT_BLOCK_NOTE_LBL"
		       description="COM_KNOWRES_CONTRACT_BLOCK_NOTE_DSC"/>
	</fieldset>
	<fieldset name="krtotals">
		<field name="contract_total"
		       type="text"
		       class="px-2 pb-1"
		       default="0"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_CONTRACT_TOTAL_LBL"
		       description="COM_KNOWRES_CONTRACT_CONTRACT_TOTAL_DSC"/>
		<field name="coupon_discount"
		       type="text"
		       class="text-end px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_COUPON_DISCOUNT_LBL"
		       description="COM_KNOWRES_CONTRACT_COUPON_DISCOUNT_DSC"/>
		<field name="deposit"
		       type="text"
		       class="text-end px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_DEPOSIT_LBL"
		       description="COM_KNOWRES_CONTRACT_DEPOSIT_DSC"/>
		<field name="deposit_system"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACTS_DEPOSIT_SYSTEM"/>
		<field name="extra_total"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       default=""
		       label="COM_KNOWRES_CONTRACT_EXTRA_TOTAL_LBL"
		       description="COM_KNOWRES_CONTRACT_EXTRA_TOTAL_DSC"/>
		<field name="tax_total"
		       type="text"
		       class="px-2 pb-1"
		       default="0"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_TAX_TOTAL_LBL"
		       description="COM_KNOWRES_CONTRACT_TAX_TOTAL_DSC"/>
		<field name="taxes"
		       type="text"
		       class="px-2 pb-1"
		       default=""
		       filter="array"
		       label="COM_KNOWRES_CONTRACT_TAXES_LBL"/>
		<field name="tag"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_TAG_LBL"
		       description="COM_KNOWRES_CONTRACT_TAG_DSC"/>
		<field name="room_total"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_ROOM_TOTAL_LBL"
		       description="COM_KNOWRES_CONTRACT_ROOM_TOTAL_DSC"/>
		<field name="room_total_gross"
		       type="text"
		       class="text-end px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_ROOM_TOTAL_GROSS_LBL"
		       description="COM_KNOWRES_CONTRACT_ROOM_TOTAL_GROSS_DSC"/>
		<field name="room_total_gross_system"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_ROOM_TOTAL_GROSS_LBL"/>
		<field name="discount"
		       type="text"
		       class="text-end px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_DISCOUNT_LBL"
		       description="COM_KNOWRES_CONTRACT_DISCOUNT_DSC"/>
		<field name="discount_system"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACTS_DISCOUNT_SYSTEM"/>
		<field name="discounts"
		       type="text"
		       class="px-2 pb-1"
		       filter="array"
		       label="COM_KNOWRES_CONTRACTS_DISCOUNTS"/>
		<field name="adjustments"
		       type="text"
		       class="px-2 pb-1"
		       filter="array"
		       label="COM_KNOWRES_CONTRACTS_ADJUSTMENTS"/>
		<field name="currency"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_CURRENCY_LBL"
		       description="COM_KNOWRES_CONTRACT_CURRENCY_DSC"/>
	</fieldset>
	<fieldset name="kradhoc">
		<field name="booking_status"
		       type="listbookingstatus"
		       class="px-2 pb-1"
		       default="1"
		       filter="integer"
		       label="COM_KNOWRES_CONTRACT_BOOKING_STATUS_LBL"
		       description="COM_KNOWRES_CONTRACT_BOOKING_STATUS_DSC"/>
		<field name="discount_details"
		       type="textarea"
		       class="px-2 pb-1"
		       label="COM_KNOWRES_CONTRACT_DISCOUNT_DETAILS_LBL"
		       description="COM_KNOWRES_CONTRACT_DISCOUNT_DETAILS_DSC"/>
		<field name="commission"
		       type="text"
		       class="px-2 pb-1"
		       default="0"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_COMMISSION_LBL"
		       description="COM_KNOWRES_CONTRACT_COMMISSION_DSC"/>
		<field name="markup"
		       type="text"
		       class="px-2 pb-1"
		       default="0"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_MARKUP_LBL"
		       description="COM_KNOWRES_CONTRACT_MARKUP_DSC"/>
		<field name="net_price_system"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_NET_PRICE_SYSTEM_LBL"
		       description="COM_KNOWRES_CONTRACT_NET_PRICE_SYSTEM_DSC"/>
		<field name="on_request"
		       type="hidden"
		       class="px-2 pb-1"
		       default="0"
		       filter="integer"
		       label="COM_KNOWRES_CONTRACT_ON_REQUEST_LBL"
		       description="COM_KNOWRES_CONTRACT_ON_REQUEST_DSC">
		</field>
		<field name="agency_id"
		       type="listforeignkey"
		       class="px-2 pb-1"
		       label="COM_KNOWRES_CONTRACT_AGENCY_ID_LBL"
		       description="COM_KNOWRES_CONTRACT_AGENCY_ID_DSC"
		       input_type="list"
		       table="#__knowres_agency"
		       key_field="id"
		       value_field="name"/>
		<field name="agent_id"
		       type="listforeignkey"
		       class="px-2 pb-1"
		       input_type="list"
		       table="#__knowres_agent"
		       key_field="id"
		       value_field="name"
		       where="state=1"
		       validate="agent"
		       label="COM_KNOWRES_CONTRACT_AGENT_ID_LBL"
		       description="COM_KNOWRES_CONTRACT_AGENT_ID_DSC"/>
		<field name="agent_reference"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_AGENT_REFERENCE_LBL"
		       description="COM_KNOWRES_CONTRACT_AGENT_REFERENCE_DSC"/>
		<field name="agent_value"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_AGENT_VALUE_LBL"
		       description="COM_KNOWRES_CONTRACT_AGENT_VALUE_DSC"/>
		<field name="agent_commission"
		       type="text"
		       class="px-2 pb-1"
		       default="0"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_AGENT_COMMISSION_LBL"
		       description="COM_KNOWRES_CONTRACT_AGENT_COMMISSION_DSC"/>
		<field name="agent_deposit_paid"
		       type="list"
		       class="px-2 pb-1"
		       default="0"
		       filter="integer"
		       label="COM_KNOWRES_CONTRACT_AGENT_DEPOSIT_PAID_LBL">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
		<field name="manager_id"
		       type="text"
		       class="px-2 pb-1"
		       filter="integer"
		       label="COM_KNOWRES_CONTRACT_MANAGER_ID_LBL"
		       description="COM_KNOWRES_CONTRACT_MANAGER_ID_DSC"/>
		<field name="service_id"
		       type="text"
		       class="px-2 pb-1"
		       filter="integer"
		       label="COM_KNOWRES_CONTRACT_SERVICE_ID_LBL"
		       description="COM_KNOWRES_CONTRACT_SERVICE_ID_DSC"/>
		<field name="channel_commission"
		       type="text"
		       class="px-2 pb-1"
		       default="0"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_CHANNEL_COMMISSION_LBL"
		       description="COM_KNOWRES_CONTRACT_CHANNEL_COMMISSION_DSC"/>
	</fieldset>
	<fieldset name="krcancelled">
		<field name="cancelled"
		       type="list"
		       class="px-2 pb-1"
		       default="0"
		       filter="integer"
		       label="COM_KNOWRES_CONTRACT_CANCELLED_LBL"
		       description="COM_KNOWRES_CONTRACT_CANCELLED_DSC">
			<option value="1">JYES</option>
			<option value="0">JNO</option>
		</field>
		<field name="cancelled_timestamp"
		       type="text"
		       class="px-2 pb-1"
		       filter="string"
		       label="COM_KNOWRES_CONTRACT_CANCELLED_TIMESTAMP_LBL"
		       description="COM_KNOWRES_CONTRACT_CANCELLED_TIMESTAMP_DSC"/>
	</fieldset>
	<fieldset name="krhidden">
		<field name="arrival"
		       type="hidden"
		       filter="string"
		       required="true"/>
		<field name="black_booking"
		       type="hidden"
		       value="0"
		       filter="integer"/>
		<field name="children"
		       type="hidden"
		       default="0"
		       filter="integer"/>
		<field name="coupon_id"
		       type="hidden"
		       filter="integer"/>
		<field name="departure"
		       type="hidden"
		       filter="string"
		       required="true"/>
		<field name="expiry_date"
		       type="hidden"
		       label="COM_KNOWRES_CONTRACT_EXPIRY_DATE_LBL"
		       description="COM_KNOWRES_CONTRACT_EXPIRY_DATE_DSC"/>
		<field name="balance_date"
		       type="hidden"
		       label="COM_KNOWRES_CONTRACT_BALANCE_DATE_LBL"
		       description="COM_KNOWRES_CONTRACT_BALANCE_DATE_DSC"/>
		<field name="nightly"
		       type="hidden"
		       filter="array"/>
		<field name="pets"
		       type="hidden"
		       default="0"
		       filter="integer"/>
		<field name="property_id"
		       type="hidden"
		       filter="int"/>
		<field name="service_ref"
		       type="text"
		       filter="string"/>
	</fieldset>
	<fieldset name="krcommon">
		<field name="state"
		       type="list"
		       class="form-select-color-state"
		       size="1"
		       default="1"
		       label="JSTATUS"
		       description="JFIELD_PUBLISHED_DESC">
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="2">JARCHIVED</option>
			<option value="-2">JTRASHED</option>
		</field>
		<field name="id"
		       type="text"
		       default="0"
		       readonly="true"
		       class="readonly"
		       label="COM_KNOWRES_ID"
		       description="JGLOBAL_FIELD_ID_DESC"/>
		<field name="created_at"
		       type="hidden"
		       filter="string"
		       label="COM_KNOWRES_CREATED_AT_LBL"
		       description="COM_KNOWRES_CREATED_AT_DSC"/>
		<field name="created_by"
		       type="user"
		       readonly="true"
		       default=""
		       label="COM_KNOWRES_CREATED_BY_LBL"
		       description="COM_KNOWRES_CREATED_BY_DSC"/>
		<field name="updated_at"
		       type="hidden"
		       filter="string"
		       label="COM_KNOWRES_UPDATED_AT_LBL"
		       description="COM_KNOWRES_UPDATED_AT_DSC"/>
		<field name="updated_by"
		       type="user"
		       readonly="true"
		       label="COM_KNOWRES_UPDATED_BY_LBL"
		       description="COM_KNOWRES_UPDATED_BY_DSC"/>
		<field name="checked_out"
		       type="hidden"
		       filter="unset"/>
		<field name="checked_out_time"
		       type="hidden"
		       filter="unset"/>
	</fieldset>
</form>